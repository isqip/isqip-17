<% include header %>


 <form class="col-md-3" action="/save-disease" method="post">
	<div class="form-group">
    	<label class="control-label">Disease Name</label>
    	<input class="form-control" type="text" name="dName"/> 
    </div>
    <div class="form-group">
    	<label class="control-label">Symptoms</label>
    	<select class="form-control" multiple name="symptomIds" >
    		<% for(var i=0; i<symptoms.length; i++){ %>
			<option value="<%= symptoms[i]._id %>"> <%= symptoms[i].symptomName %> </option>
    		<% } %>
    	</select>
    </div>
    <div class="form-group">
    	<label class="control-label">Fatal?</label>
    	<select class="form-control" name="fatal">
    		<option value="yes">No</option>
    		<option value="no">Yes</option>
    	</select>
    </div>
    	<button class="pull-right btn btn-info" type="submit">Submit</button>
    </form>
<% include footer %>
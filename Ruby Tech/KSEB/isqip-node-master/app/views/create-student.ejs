<% include header %>
	<form action="/student/submit-form" method="post" class="col-md-6 center">
		<div class="form-group">
			<label class="control-label">Consumer Name</label>
			<input class="form-control" type="text" name="studentName" placeholder="Enter Consumer Name" />
		</div>

		<div class="form-group">
			<label class="control-label">House Name</label>
			<input class="form-control" type="text" name="className" placeholder="Enter House Name" />
		</div>

		<div class="form-group">
			<label class="control-label">House Number</label>
			<input class="form-control" type="text" name="rollNum" placeholder="Enter House Number" />
		</div>

		<div class="form-group">
			<label class="control-label">Contact Number</label>
			<input class="form-control" type="text" name="dOb" placeholder="Enter Contact Number" />
		</div>

		<div class="form-group">
			<label class="control-label">Units in kWh</label>
			<input class="form-control" type="text" name="Units" placeholder="Enter Units in kWh" />
		</div>

		<div class="form-group">
			<label class="control-label">I Agree: </label>
			<input type="checkbox" name="isClassLeader"/>
		</div>
		<a href="/" class="btn btn-danger">Cancel</a>
		<button type="submit" class="btn btn-info pull-right">Create</button>
	</form>
<% include footer %>